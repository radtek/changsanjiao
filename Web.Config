<system.webServer>
    <validation validateIntegratedModeConfiguration="false" />    
    <handlers>
      <add name="dd" path="*.do" verb="*" type="ExtHandler.PatrolHandler, ExtHandler" />
      <add name="ee" path="*.asyn" verb="POST,GET" type="CometHandler.AsnyHandler,CometHandler" />
